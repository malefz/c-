/*
Ejercicio 8: Uso de continue en Bucles
Crea un programa que imprima los números del 1 al 20, pero omita los múltiplos de 3 utilizando la sentencia continue.
*/

#include <iostream>

using namespace std;

int main()
{
    for (int i = 1; i <= 20; i++) //en esta iteracion i= 3
    {
        if (i % 3 == 0)   //cuando se cumple la condicion
        {
            continue; //interrumpe esta iteracion
        }
        cout << i << endl; //no me permite la impresion del valor 3
    }
    return 0;
}
